{
  "extends": "airbnb/base",
  "rules": {
    "comma-dangle": [2, "never"],
    "func-names": 0,
    "strict": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-vars": 1,
    "vars-on-top": 1,
    "id-length": [2, {"properties": "never", "exceptions": ["i", "j", "k", "l", "x", "y", "_"]}],
    "indent": [2, 2, {"SwitchCase": 1}],
    "padded-blocks": 0,
    "brace-style": 0,
    "no-console": 0
  },
  "globals": {
    "angular": true,
    "_": true
  }
}
